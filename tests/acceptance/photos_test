var App;

module("Acceptances - Photos", {
  setup: function(){
    App = startApp();
  },
  teardown: function() {
    deleteDB(App);    
    Ember.run(App, 'destroy');
  }
});

asyncTest("index renders", function(){
  expect(1);
  visit('/photos').then(function(){
    ok(exists(".btn.new:contains('New photo')"));
    start();
  });
});
